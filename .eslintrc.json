{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "plugins": [
    "lodash"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:lodash/recommended"
  ],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "ignorePatterns": [
    "jquery/**",
    "package.*?.json",
    "bootstrap/**",
    "**/node-modules/*"
  ],
  "rules": {}
}